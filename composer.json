{
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.23",
        "phpstan/phpstan": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "infection/infection": "^0.26.10",
        "phpunit/php-code-coverage": "^9.2",
        "qossmic/deptrac-shim": "^0.20.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
